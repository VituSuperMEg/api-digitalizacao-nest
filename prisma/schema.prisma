generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Users {
  id Int @id @default(autoincrement())
  login String
  senha String
  ativo String
  email String @unique
  telefone String @unique
  criado_em DateTime @default(now())
  alterado_em DateTime @default(now())
  @@map("users")
}
